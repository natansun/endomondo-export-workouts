{
  "name": "Endomondo Workouts Downloader",
  "description": "Download your Endomondo workouts as TCX/GPX files",
  "version": "1.0.3",
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": [
      "jquery-3.5.1.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["https://*.endomondo.com/*"],
      "run_at": "document_end",
      "js": [
        "jquery-3.5.1.min.js",
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs",
    "https://*.endomondo.com/*",
    "downloads"
  ],
  "icons": {
    "48": "icons/blue-48.png"
  },
  "manifest_version": 2,
  "externally_connectable": {
    "matches": ["https://*.endomondo.com/*"]
  }
}
